{
  "name": "cashweb-client",
  "version": "0.0.1",
  "description": "Personal Finance tool based on GNUCash",
  "main": "./config/webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production webpack --progress --profile --colors --production --config ./config/webpack.config.js",
    "serve-dev": "NODE_ENV=development webpack -w --progress --colors --config ./config/webpack.config.js",
    "serve-hot": "NODE_ENV=development webpack-dev-server --progress --colors --config ./config/webpack.config.js",
    "start:env": "zsh source ./venv/bin/activate",
    "start:dev": "FLASK_DEBUG=1 FLASK_APP=$(pwd)/cashweb/server/__init__.py flask run",
    "serve": "FLASK_APP=cashweb/server/__init__.py flask run"
  },
  "keywords": [
    "finance"
  ],
  "author": "j.bellero@dreamrocksmarttech.com",
  "repository": "https://github.com/jbool24/CashWEB",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "debug": "^3.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "react-router-dom": "^4.2.2",
    "semantic-ui-less": "^2.3.0",
    "semantic-ui-react": "^0.78.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-export-default-from": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-react": "^7.0.0-beta.40",
    "autoprefixer": "^8.0.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.17.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "less": "^3.0.1",
    "less-loader": "^4.0.6",
    "less-plugin-rewrite-import": "^0.1.1",
    "manifest-revision-webpack-plugin": "^0.4.1",
    "node-sass": "^4.7.2",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "react-dev-utils": "^5.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-merge": "^4.1.1"
  }
}
